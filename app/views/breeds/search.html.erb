<p class="mb-5">Found <%= pluralize(@breeds.count, 'Breed') %> when searching for: <%= params[:keywords] %></p>

<ul>
  <% @breeds.each do |breed| %>
    <li>
      Breed name: <%= link_to breed.name, breed %>, 
        <ul>
          <% breed.dogs.each do |dog| %>
            <li>Dog name: <%= dog.name %></li>
          <% end %>
        </ul>
    </li>
  <% end %>
</ul>